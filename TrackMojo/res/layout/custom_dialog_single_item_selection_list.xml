<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/dialog_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/tendp"
        android:background="@android:color/white"
        android:text="@string/activity_add_personal_trac_custom_trac_ideas_title"
        android:textColor="@color/blue_button_default_color"
        android:textSize="@dimen/font_large" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/onedp"
        android:background="@color/list_text">

        </View>

    <ListView
        android:id="@+id/custom_choose_quantity_lv_range"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    </ListView>

</LinearLayout>