<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:circleview="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/scrollView3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:scrollbars="none">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <!--<TextView
            android:id="@+id/fragment_trac_rate_tvTracRate"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_home_list_header"
            android:gravity="center"
            android:text="@string/fragment_trac_rate"
            android:textColor="@color/blue_button_default_color"
            android:textSize="@dimen/font_medium" />-->

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="@dimen/tendp"
            android:layout_marginLeft="@dimen/tendp"
            android:layout_marginRight="@dimen/tendp"
            android:background="@drawable/edit_box_background_solid"
            android:padding="@dimen/tendp">
            
            
            
            <TextView
                android:id="@+id/row_home_list_item_business_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                
                android:ellipsize="end"
                android:singleLine="true"
                android:text="sdfas"
                android:visibility="gone"
                android:textColor="#ed7070"
                android:textSize="@dimen/font_micro" />
            
            <TextView
                android:id="@+id/fragment_trac_rate_tvGroupName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                
                android:ellipsize="end"
                android:singleLine="true"
                android:text="sdfas"
                android:textSize="@dimen/font_micro" />

            <TextView
                android:id="@+id/fragment_trac_rate_tvTracName"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/fragment_trac_rate_tvGroupName"
                android:layout_marginTop="@dimen/fivedp"
                android:ellipsize="end"
                android:maxLines="2"
                android:text="sdfas\nsdljsdkfs ;s df    slfjsd"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_small" />
        </LinearLayout>

        <RelativeLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginTop="@dimen/_10sdp">

            <ImageView
                android:id="@+id/fragment_trac_rate_adCustomCanvas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="@dimen/_30sdp"
                android:src="@drawable/ic_trac_rate_selection" />

            <ImageView
                android:id="@+id/fragment_trac_rate_cdSelectedTracRate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_5sdp"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_custom_rate_zero" />

            <ImageView
                android:id="@+id/fragment_trac_rate_cdTracOne"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="@dimen/_64sdp"
                android:src="@android:color/transparent" />

            <TextView
                android:id="@+id/fragment_trac_rate_tvTracOption1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/fragment_trac_rate_cdTracOne"
                android:layout_alignTop="@+id/fragment_trac_rate_cdTracOne"
                android:layout_marginLeft="@dimen/_80sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:ellipsize="end"
                android:maxLines="2"
                android:paddingRight="@dimen/tendp"
                android:text="nikunj nikunj"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:id="@+id/fragment_trac_rate_cdTracSecond"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_below="@+id/fragment_trac_rate_cdTracOne"
                android:layout_marginLeft="@dimen/_90sdp"
                android:src="@android:color/transparent" />

            <TextView
                android:id="@+id/fragment_trac_rate_tvTracOption2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/fragment_trac_rate_cdTracSecond"
                android:layout_alignTop="@+id/fragment_trac_rate_cdTracSecond"
                android:layout_marginLeft="@dimen/_78sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="abc"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:id="@+id/fragment_trac_rate_cdTracThree"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_below="@+id/fragment_trac_rate_cdTracSecond"
                android:layout_marginLeft="@dimen/_45sdp"
                android:layout_toRightOf="@+id/fragment_trac_rate_cdSelectedTracRate"
                android:src="@android:color/transparent" />


            <TextView
                android:id="@+id/fragment_trac_rate_tvTracOption3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/fragment_trac_rate_cdTracThree"
                android:layout_alignTop="@+id/fragment_trac_rate_cdTracThree"
                android:layout_marginLeft="@dimen/_70sdp"
                android:layout_marginTop="@dimen/_15sdp"
                android:ellipsize="end"
                android:maxLines="2"
                android:paddingRight="@dimen/_3sdp"
                android:text="nikunj nikunj"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:id="@+id/fragment_trac_rate_cdTracFour"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/fragment_trac_rate_cdTracThree"
                android:layout_marginLeft="@dimen/onetwenty"
                android:src="@android:color/transparent" />

            <TextView
                android:id="@+id/fragment_trac_rate_tvTracOption4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/fragment_trac_rate_cdTracFour"
                android:layout_alignTop="@+id/fragment_trac_rate_cdTracFour"
                android:layout_marginLeft="@dimen/_65sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="tus"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_small" />

            <ImageView
                android:id="@+id/fragment_trac_rate_cdTracFive"
                android:layout_width="@dimen/_90sdp"
                android:layout_height="@dimen/_50sdp"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/fragment_trac_rate_cdTracFour"
                android:layout_marginLeft="@dimen/_70sdp"

                android:src="@android:color/transparent" />

            <TextView
                android:id="@+id/fragment_trac_rate_tvTracOption5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/fragment_trac_rate_cdTracFive"
                android:layout_alignTop="@+id/fragment_trac_rate_cdTracFive"
                android:layout_marginLeft="@dimen/_62sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="push"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_small" />
        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/tendp"
            android:gravity="center_horizontal"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/fragment_trac_rate_tvFrequency"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/fivedp"
                android:gravity="center_horizontal"
                android:singleLine="true"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_micro" />

            <TextView
                android:id="@+id/fragment_trac_rate_tvLastRated"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/fivedp"
                android:gravity="center_horizontal"
                android:singleLine="true"
                android:textColor="@color/edit_box_phone"
                android:textSize="@dimen/font_micro" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2"
            android:layout_marginTop="@dimen/tendp"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/fragment_trac_rate_tvComment"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:background="@color/edit_box_back"
                android:gravity="center_horizontal"
                android:padding="@dimen/tendp"
                android:text="@string/fragment_trac_rate_comment"
                android:enabled="false"
                android:textColor="@drawable/text_selector_comment"
                android:textSize="@dimen/font_medium" />
            <TextView
                android:id="@+id/fragment_trac_rate_tvDone"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:background="@color/edit_box_back"
                android:gravity="center_horizontal"
                android:padding="@dimen/tendp"
                android:text="@string/fragment_trac_rate_done"
                android:textColor="@color/blue_button_default_color"
                android:textSize="@dimen/font_medium" />
        </LinearLayout>
    </LinearLayout>
</ScrollView>