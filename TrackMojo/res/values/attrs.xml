<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="circleview">
        <attr name="cRadius" format="integer" />
        <attr name="cFillColor" format="color" />
        <attr name="cStrokeColor" format="color" />
        <attr name="cAngleStart" format="integer" />
        <attr name="cAngleEnd" format="integer" />
        <attr name="cIsShadow" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CircularImageView">
        <attr name="border" format="boolean"></attr>
        <attr name="border_width" format="dimension"></attr>
        <attr name="border_color" format="color"></attr>
        <attr name="shadow" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="Theme">
        <attr name="circularImageViewStyle" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="CustomTextView">
        <attr name="fontName" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="fontNameForButton" format="string" />
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="fontNameForEditText" format="string" />
    </declare-styleable>
</resources>